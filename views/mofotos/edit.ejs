<%- layout('layouts/boilerplate') %>
<h1>Edit Mofoto</h1>
<form action="/mofotos/<%= mofoto._id %>?_method=PUT" method="POST">
  <div>
    <label for="title">Title</label>
    <input
      type="text"
      id="title"
      name="mofoto[title]"
      value="<%= mofoto.title %>"
    />
  </div>
  <div>
    <label for="location">Location</label>
    <input
      type="text"
      id="location"
      name="mofoto[location]"
      value="<%= mofoto.location %>"
    />
  </div>
  <button>Update Mofoto</button>
</form>
