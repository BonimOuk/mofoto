<%- layout('layouts/boilerplate') %>
    <h1><%= mofoto.title %></h1>
    <h2><%= mofoto.location %></h2>
    <img src="<%= mofoto.image %>" alt="">
    <p><%= mofoto.description %></p>
    <a href="/mofotos/<%= mofoto._id %>/edit">Edit</a>
    <p>
      <form action="/mofotos/<%= mofoto._id%>?_method=DELETE" method="POST"><button>Delete</button></form>
    </p>
    <footer>
      <a href="/mofotos">All Mofotos</a>
    </footer>

